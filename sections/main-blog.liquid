{{ 'section-blog.css' | asset_url | stylesheet_tag }}
{% render 'log', object:blog.article_count %}
<div class="blog">
  <h1 class="blog-header">{{blog.title}}</h1>
  <div class="blog-posts">
    {% for article in blog.articles %}
      <div class="blog-post">
        <div class="blog-post-image">
        </div>
        <h2 class="blog-post-title">{{article.title}}</h2>
        <p class="blog-post-body">
          {{article.body}}
        </p>
      </div>
    {% endfor %}
  </div>
</div>